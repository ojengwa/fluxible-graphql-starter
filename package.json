{
  "name": "mesiac",
  "version": "0.0.1",
  "private": true,
  "main": "start.js",
  "scripts": {
    "build": "webpack & webpack --config webpack.config.production.js",
    "db:migrate": "knex migrate:latest --knexfile ./db/knexfile.js",
    "db:migrate:make": "knex migrate:make --knexfile ./db/knexfile.js",
    "db:migrate:seed": "yarn db:migrate && yarn db:seed",
    "db:seed": "knex seed:run --knexfile ./db/knexfile.js",
    "db:seed:make": "knex seed:make --knexfile ./db/knexfile.js",
    "dev": "node webpack-dev-server.js",
    "lint": "eslint --fix ./*.js ./**/*.js",
    "test": "jest --color=true --detectOpenHandles --forceExit",
    "start": "node start.js"
  },
  "jest": {
    "automock": true
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.6.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.6.0",
    "body-parser": "^1.6.4",
    "bookshelf": "^0.14.2",
    "compression": "^1.5.1",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "debug": "^2.0.0",
    "express": "^4.3.2",
    "express-graphql": "^0.7.1",
    "express-state": "^1.2.0",
    "fluxible": "^1.0.0",
    "fluxible-addons-react": "^0.2.0",
    "fluxible-plugin-fetchr": "^0.3.11",
    "fluxible-router": "^0.4.0",
    "graphql": "^14.0.2",
    "graphql-bookshelfjs": "^1.0.8",
    "knex": "^0.16.3",
    "lodash": "^4.17.11",
    "mysql": "^2.16.0",
    "react": "^15.0.0-rc.1",
    "react-dom": "^15.0.0-rc.1",
    "react-relay": "^1.7.0",
    "relay-runtime": "^1.7.0",
    "serialize-javascript": "^1.0.0",
    "serve-favicon": "^2.1.6"
  },
  "devDependencies": {
    "@mesaic/eslint-config-mesaic": "^0.5.0",
    "@mesaic/mesaic-codeformat": "^3.0.0",
    "babel-eslint": "^5.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-relay": "^1.7.0",
    "bundle-loader": "^0.5.0",
    "eslint": "^2.2.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.1.0",
    "file-loader": "^3.0.1",
    "jest": "^23.6.0",
    "json-loader": "^0.5.1",
    "knex-seeder": "https://github.com/2rhop/knex-seeder#eb2069e7042cb4af625d004820e28678803560b3",
    "nodemon": "^1.2.1",
    "react-hot-loader": "^1.2.8",
    "shelljs": "^0.6.0",
    "url-loader": "^1.1.2",
    "webpack": "^1.12.4",
    "webpack-dev-server": "^1.6.5"
  },
  "engine": "10.14"
}
